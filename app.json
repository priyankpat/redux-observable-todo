{
  "expo": {
    "name": "Redux Todo App",
    "slug": "redux-todo-app",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["./assets/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "Sample app for TODO built in Expo, Redux and Observables",
    "githubUrl": "https://github.com/priyankpat/redux-observable-todo"
  }
}
